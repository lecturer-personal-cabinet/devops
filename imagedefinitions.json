{
  "taskDefinition": {
    "containerDefinitions": [
      {
        "name": "frontend",
        "imageUri": "uladzimirbaklan/lecturer-private-cabinet-frontend:latest",
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 3000,
            "hostPort": 80
          }
        ],
        "memory": 300
      },
      {
        "name": "backend",
        "imageUri": "uladzimirbaklan/lecturer-private-cabinet-backend:latest",
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 8080,
            "hostPort": 8080
          }
        ],
        "memory": 300
      }
    ]
  }
}
